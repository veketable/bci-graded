            "examples": [
                "1234"
            ]
        },
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The email schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "veke.table@gmail.com"
            ]
        },
        "phone": {
            "$id": "#/properties/phone",
            "type": "string",
            "title": "The phone schema",
            "description": "An explanation about the purpose of this instance.",
